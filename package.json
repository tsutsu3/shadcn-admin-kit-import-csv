{
  "name": "shadcn-admin-kit-import-csv",
  "version": "0.1.0",
  "description": "CSV/TSV import button for react-admin with shadcn/ui components. A fork of react-admin-import-csv with Material UI replaced by shadcn/ui + Tailwind CSS.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./i18n": {
      "import": {
        "types": "./dist/i18n/index.d.ts",
        "default": "./dist/i18n/index.js"
      },
      "require": {
        "types": "./dist/i18n/index.d.cts",
        "default": "./dist/i18n/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-admin",
    "csv",
    "tsv",
    "import",
    "shadcn",
    "shadcn-ui",
    "tailwindcss",
    "radix-ui",
    "dataprovider"
  ],
  "author": "tsutsu3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsutsu3/shadcn-admin-kit-import-csv.git"
  },
  "homepage": "https://github.com/tsutsu3/shadcn-admin-kit-import-csv",
  "bugs": {
    "url": "https://github.com/tsutsu3/shadcn-admin-kit-import-csv/issues"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": ">=0.300.0",
    "papaparse": "^5.0.0",
    "ra-core": "^5.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "tailwind-merge": "^3.0.0"
  },
  "dependencies": {
    "papaparse": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.0.0"
  }
}
